#: TutoPrev
msgctxt "TutoPrev"
msgid "Prev"
msgstr "Anterior"

#: TutoNext
msgctxt "TutoNext"
msgid "Next"
msgstr "Próximo"

#: TutoEnd
msgctxt "TutoEnd"
msgid "End"
msgstr "Fim"

#: TutoExplainTitle
msgctxt "TutoExplainTitle"
msgid "Video Viewer Activity"
msgstr "Atividade Visualizar Vídeo"

#: TutoExplainContent
msgctxt "TutoExplainContent"
msgid "Welcome to the Video Viewer Activity. A browser and a viewer for Khan Academy, Canopé videos and more."
msgstr "Welcome to the Video Viewer Activity. A browser and a viewer for Khan Academy, Canopé videos and more."

#: TutoFilterTitle
msgctxt "TutoFilterTitle"
msgid "Filter"
msgstr "Filter"

#: TutoFilterContent
msgctxt "TutoFilterContent"
msgid "Click here to filter the videos by category"
msgstr "Click here to filter the videos by category"

#: TutoFavoriteTitle
msgctxt "TutoFavoriteTitle"
msgid "Favorite videos"
msgstr "Vídeos favoritos"

#: TutoFavoriteContent
msgctxt "TutoFavoriteContent"
msgid "Click here to see the videos marked by you as favorite"
msgstr "Click here to see the videos marked by you as favorite"

#: TutoLibraryTitle
msgctxt "TutoLibraryTitle"
msgid "Libraries"
msgstr "Bibliotecas"

#: TutoLibraryContent
msgctxt "TutoLibraryContent"
msgid "Click here to choose between Khan Academy, Canopé videos or to add your own library"
msgstr "Click here to choose between Khan Academy, Canopé videos or to add your own library"

#: TutoExportvideoTitle
msgctxt "TutoExportvideoTitle"
msgid "Export videos"
msgstr "Exportar vídeos"

#: TutoExportvideoContent
msgctxt "TutoExportvideoContent"
msgid "Click here to save a video in the Journal. First click here then click on the video you want to save"
msgstr "Click here to save a video in the Journal. First click here then click on the video you want to save"

#: TutoVideoTitle
msgctxt "TutoVideoTitle"
msgid "Video"
msgstr "Video"

#: TutoVideoContent
msgctxt "TutoVideoContent"
msgid "Click here to play the video"
msgstr "Clicar aqui para executar um vídeo"

#: TutoSearchTitle
msgctxt "TutoSearchTitle"
msgid "Search"
msgstr "Pesquisar"

#: TutoSearchContent
msgctxt "TutoSearchContent"
msgid "Type here to search for a video by its name"
msgstr "Digite aqui para pesquisar um vídeo pelo seu nome"
